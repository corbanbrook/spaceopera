void setup() {
  size(500, 500);
} //setup

void draw() { 
  fill(0, 121, 184); 
  stroke(255); 
  background(10, 10, 10, 10); 

  if (players.length > 0) { 
    for (var p in players) { 
      console.log("here");
      var player = players[p]; 
      ellipse(player.position.x, player.position.y, 10, 10); 
    } 
  } 
} //draw

